{
  "expo": {
    "name": "gasstation",
    "description": "current price of gas for Etherium transactions",
    "slug": "gasstation",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
